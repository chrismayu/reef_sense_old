<%= simple_form_for @arduino, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :reef_tank_id %>
  <%= f.input :parameter_setup, :input_html => { :class => "enroll_input span4", :rows => 2}  %>
  <%= f.input :reef_tank_arduino_id %>
  <%= f.input :arduino_board_type %>
  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                arduinos_path, :class => 'btn' %>
  </div>
<% end %>
