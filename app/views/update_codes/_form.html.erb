<%= simple_form_for(@update_code) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :reef_tank_id %>
    <%= f.input :reef_tank_arduino_id %>
    <%= f.input :sent_email %>
    <%= f.input :message %>
    <%= f.input :priority %>
    <%= f.input :display_site_wide %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
