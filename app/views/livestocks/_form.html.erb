<%= simple_form_for(@livestock) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :scientific_name %>
    <%= f.input :image %>
    <%= f.input :description %>
    <%= f.input :livestock_type %>
    <%= f.input :common_name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
