<%= simple_form_for(@reef_tank) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :user_id %>
    <%= f.input :setup_date %>
    <%= f.input :size %>
    <%= f.input :measurement_type %>
    <%= f.input :send_email_okay %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
